<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>


<HBox spacing="5.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button fx:id="music" mnemonicParsing="false" prefHeight="46.0" prefWidth="49.0" style="-fx-background-color: white;">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0">
               <image>
                  <Image url="@../images/Audio.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="image" mnemonicParsing="false" prefHeight="46.0" prefWidth="49.0" style="-fx-background-color: white;">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0">
               <image>
                  <Image url="@../images/image-gallery.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="document" mnemonicParsing="false" prefHeight="46.0" prefWidth="49.0" style="-fx-background-color: white;">
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="40.0">
               <image>
                  <Image url="@../images/google-docs.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</HBox>
