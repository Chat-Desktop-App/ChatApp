<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="220.0" prefWidth="382.0" type="HBox" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gov.iti.jets.client.allFriendController">
   <children>
      <ImageView fx:id="friendIcon" fitHeight="40.0" fitWidth="40.0">
         <image>
            <Image url="@../images/friendIcon.png" />
         </image>
         <HBox.margin>
            <Insets left="30.0" top="40.0" />
         </HBox.margin>
      </ImageView>
      <Circle fx:id="status" fill="#6e716f99" radius="5.0" stroke="#00000007" strokeType="INSIDE">
         <HBox.margin>
            <Insets left="-9.0" top="42.0" />
         </HBox.margin>
      </Circle>
      <Label fx:id="friendName" text="Omer">
         <HBox.margin>
            <Insets left="8.0" top="48.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
</fx:root>
